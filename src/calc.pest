main = { SOI ~ expr ~ EOI}
expr = {factor ~ ((addop ~ factor) | (subop ~ factor))* }
factor = { unary ~ ( (mulop ~ unary) | (divop ~ unary) )* }
unary = { (addop | subop)? ~ atom }
atom = { num | "(" ~ expr ~ ")" }
num = @{ASCII_DIGIT+}
addop = { "+" }
subop = { "-" }
mulop = { "*" }
divop = { "/" }

WHITESPACE = _{ " " }
